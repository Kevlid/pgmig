{
    "name": "pgmig",
    "version": "0.0.0",
    "description": "A package for handling PostgreSQL migrations",
    "author": "Kevlid",
    "license": "MIT",
    "keywords": [
        "typescript",
        "postgres",
        "migrations",
        "sql"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rm -rf ./dist"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Kevlid/pgmig.git"
    },
    "bugs": {
        "url": "https://github.com/Kevlid/pgmig/issues"
    },
    "homepage": "https://github.com/Kevlid/pgmig#readme",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "pg": "^8.16.3"
    },
    "devDependencies": {
        "@types/node": "^24.6.2",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=16"
    },
    "packageManager": "pnpm@10.18.2"
}
